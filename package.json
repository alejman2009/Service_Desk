{
  "name": "service_desk",
  "version": "1.0.0",
  "description": "My SAP project",
  "repository": "https://github.com/alejman2009/Service_Desk.git",
  "license": "AFL-3.0",
  "private": true,
  "dependencies": {
    "@cap-js/hana": "^1",
    "@sap-cloud-sdk/http-client": "^3.26.0",
    "@sap-cloud-sdk/util": "^3.26.0",
    "@sap/cds": "^8",
    "@sap/low-code-event-handler": "^1.2.5",
    "@sap/mdk-tools": "^1.12.1",
    "@sap/xsenv": ">=3.3.2 <3.3.3",
    "@sap/xssec": "^4.2.8",
    "express": "^4",
    "hdb": "^0.19.12",
    "passport": "^0.7.0"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.8.0",
    "@cap-js/sqlite": "^1",
    "@sap/dev-cap-tools": "^1.31.0"
  },
  "scripts": {
    "start": "cds-serve",
    "start-local": "npm run generate-entry-point && npm run cds-watch",
    "generate-entry-point": "dev-cap-tools gen-entrypoint",
    "cds-watch": "cds watch --open",
    "watch-service_desk.service_desk_admin_app": "cds watch --open service_desk.service_desk_admin_app/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-service_desk.service_desk_user_app": "cds watch --open service_desk.service_desk_user_app/webapp/index.html?sap-ui-xx-viewCache=false"
  },
  "cds": {
    "sql": {
      "native_hana_associations": false
    },
    "requires": {
      "[production]": {
        "db": "hana",
        "auth": "xsuaa"
      },
      "app-service": {
        "impl": "@sap/low-code-event-handler"
      },
      "auth": {
        "[development]": {
          "kind": "dummy"
        },
        "[production]": {
          "kind": "xsuaa"
        }
      }
    }
  },
  "lcap": true,
  "sapux": [
    "app/service_desk.service_desk_admin_app",
    "app/service_desk.service_desk_user_app"
  ]
}
